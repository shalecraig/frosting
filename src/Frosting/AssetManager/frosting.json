{    
    "imports": [
        "../FileSystem/frosting.json"
    ],
    "services": {
        "assetManager": {
            "class": "Frosting\\AssetManager\\Manager",
            "configuration": {
                "rootDirectory": null,
                "aggregation": false
            }
        },
        "urlBuilder": {
            "class": "Frosting\\AssetManager\\UrlBuilder"
        },
        "assetManagerTwigExtension": {
            "class": "Frosting\\AssetManager\\Twig\\AssetManagerExtension"
        },
        "assetManagerFilePersister": {
            "class": "Frosting\\AssetManager\\FilePersister"
        }
    }
}
